<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.phlink.bus.api.bus.dao.TeacherCheckoutEndMapper">

    <select id="getCheckoutByDay" resultType="com.phlink.bus.api.bus.domain.TeacherCheckoutEnd">
		select *
		from t_teacher_checkout_end
		where user_id = #{userId}
		and check_date = #{day}
		limit 1
    </select>
</mapper>
